{
  "name": "@justack/sdk",
  "version": "0.1.1",
  "description": "TypeScript SDK for the Justack Human-in-the-Loop API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/errors.d.ts",
        "default": "./dist/errors.js"
      },
      "require": {
        "types": "./dist/errors.d.cts",
        "default": "./dist/errors.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "check-types": "tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-turbo": "^2.7.1",
    "tsup": "^8.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.50.0",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "justack",
    "human-in-the-loop",
    "ai-agents",
    "realtime",
    "websocket",
    "sdk"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/justack-dev/typescript-sdk.git"
  },
  "homepage": "https://github.com/justack-dev/typescript-sdk#readme",
  "bugs": {
    "url": "https://github.com/justack-dev/typescript-sdk/issues"
  }
}
